{
  "name": "express-admin-honeypot",
  "version": "0.0.1",
  "description": "An advanced honeypot package for Node.js admin endpoints, inspired by django-admin-honeypot",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.12.0"
  },
  "engineStrict": true,
  "scripts": {
    "tsc": "tsc",
    "dev": "tsx --watch ./src/*.ts",
    "prebuild": "npm run tsc",
    "build": "tsup",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "test": "vitest run --coverage --passWithNoTests",
    "test:watch": "vitest dev",
    "prelint": "biome format --write ./src/*.ts ./tests/*.ts",
    "lint": "biome lint ./src/*.ts ./tests/*.ts",
    "check": "biome check --write ./src/*.ts ./tests/*.ts",
    "prepare": "husky",
    "commit": "cz"
  },
  "author": "Ali nazari",
  "license": "MIT",
  "keywords": [
    "honeypot",
    "admin",
    "security",
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.13",
    "@vitest/coverage-istanbul": "^3.0.9",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
